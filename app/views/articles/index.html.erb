<div class="container">
<center>
<h1>Article#index</h1>
<% @articles.each do |article| %>
  <h2> <%= link_to article.title, article_path(article) %> </h2>
  <%= link_to "Show more details", article_path(article) %>
  <%= link_to "Edit the article", edit_article_path(article) %>
  <%= link_to "Delete the article", article_path(article), method: :delete %>
<% end %>
<br>
<br>
<%= link_to "Create a new article", new_article_path %>
</center>
</div>
